{
	"name": "n8n-nodes-kanban-app",
	"version": "1.0.0",
	"description": "n8n node for Kanban Task Management Application",
	"keywords": [
		"n8n-community-node-package",
		"kanban",
		"task-management",
		"productivity"
	],
	"license": "MIT",
	"homepage": "https://github.com/Drmusab/Kanban-Routine-Manager",
	"author": {
		"name": "Drmusab",
		"email": "drmusab@example.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Drmusab/Kanban-Routine-Manager.git"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes --write",
		"lint": "eslint nodes package.json",
		"lintfix": "eslint nodes package.json --fix",
		"prepublishOnly": "npm run build && npm run lint -s"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/KanbanAppApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/KanbanApp/KanbanApp.node.js",
			"dist/nodes/KanbanApp/KanbanAppTrigger.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "^5.45.0",
		"eslint-plugin-n8n-nodes-base": "^1.11.0",
		"gulp": "^4.0.2",
		"n8n-workflow": "*",
		"prettier": "^2.7.1",
		"typescript": "^4.8.4"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
